total_angka = 100
panjang_baris = 10
banyak_dor = 0
for angka in range(1, total_angka + 1, panjang_baris):
    for i in range(angka, angka + panjang_baris):
        if i % 3 == 0 or i % 5 == 0:
            print("DOR", end=" ")
            banyak_dor += 1
        else:
            print(i, end=" ")
    print()
print()
print('Jumlah DOR yang muncul sebanyak=', banyak_dor)
print()
